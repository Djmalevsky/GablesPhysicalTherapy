    var flickityViewport = $('.flickity-viewport');

    if (elem == "Read More") {
      $(this).text("Read Less");
      textElement.slideDown();
      parentItem.animate({'min-height': '500px'}, 100, function() {
        parentItem.css('height', '500px !important');
        flickityViewport.animate({'height': '800px'}, 100);
      });
    } else {
      $(this).text("Read More");
      textElement.slideUp();
      parentItem.animate({'min-height': '300px'}, 100, function() {
        parentItem.css('height', '');
        flickityViewport.animate({'height': '600px'}, 100);
      });
    }
  });
});
